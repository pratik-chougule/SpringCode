    <!--  <value>201</value>
     </property> -->
     
      <property name="studentName" value="Pratik" />
      
    <!--  <value>Pratik</value>
     </property> -->
     
     <property name="studentAddress" value="Kolhapur" />
     
    <!--  <value>Kolhapur</value>
    </property>--> 
    
 </bean>
 
